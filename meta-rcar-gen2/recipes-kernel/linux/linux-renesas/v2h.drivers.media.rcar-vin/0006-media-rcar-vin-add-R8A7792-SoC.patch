+++ b/drivers/media/platform/rcar-vin/rcar-core.c
@@ -205,6 +205,7 @@ done:
 static const struct of_device_id rvin_of_id_table[] = {
 	{ .compatible = "renesas,vin-r8a7794", .data = (void *)RCAR_GEN2 },
 	{ .compatible = "renesas,vin-r8a7793", .data = (void *)RCAR_GEN2 },
