+
+	} else {
+
+
+#ifdef	DDR_DEBUG_ODTMODE
+		md = ((*((volatile uint32_t*)RST_MODEMR) & 0x00020000) >> 17)|
+		     ((*((volatile uint32_t*)RST_MODEMR) & 0x00080000) >> 18);
+//		NOTICE("BL2: DDR3200(ODTdbg:%x)\n",md);
