+			if (md == 0x0) {
+//				NOTICE("BL2: DDR3200\n");
+			} else if (md == 0x1) {
+				freq = DDR2800_CLK;
+//				NOTICE("BL2: DDR2800\n");
+			} else if (md == 0x4) {
+				freq = DDR2400_CLK;
+//				NOTICE("BL2: DDR2400\n");
+			} else {
+				freq = DDR1600_CLK;
+//				NOTICE("BL2: DDR1600\n");
+			}
+		}
