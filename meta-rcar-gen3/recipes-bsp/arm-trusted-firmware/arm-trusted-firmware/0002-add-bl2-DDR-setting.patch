+static void WaitDBCMD(void)
+{
+	uint32_t dataL;
+
+	while(1)
+	{
+		dataL = *((volatile uint32_t*)DBSC_DBWAIT);	//wait DBCMD 1=busy, 0=ready
+		if((dataL & 0x00000001) == 0x0)	break;
+	}
+}
