+static inline int32_t vch_nxt(int32_t pos)
+{
+	int32_t posn;
+	for(posn=pos;posn<DRAM_CH_CNT; posn++){
+		if(ddr_phyvalid & (1<<posn))break;
+	}
+	return posn;
+}
+#define foreach_vch(ch) \
