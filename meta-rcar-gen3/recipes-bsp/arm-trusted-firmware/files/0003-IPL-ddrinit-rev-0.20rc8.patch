+		else if(code<=0x20)
+			tmp = ((CODE_MAX-1-(0x20-code)*2)<<6) | (CODE_MAX-1);
+		else
+			tmp = ((CODE_MAX-1)<<6) | (CODE_MAX-1-(code-0x20)*2);
+	} else {
+		if(code==0)
+			tmp = (1<<6) | (CODE_MAX-1);
+		else
+			tmp = (code<<6) | (CODE_MAX-code);
+	}
+	return tmp;
+}
