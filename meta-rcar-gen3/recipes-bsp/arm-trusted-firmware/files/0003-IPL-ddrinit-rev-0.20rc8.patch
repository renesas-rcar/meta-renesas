+
+	/***********************************************************************
+	exec pi_training
+	***********************************************************************/
+	ddr_setval_ach_as(_reg_PHY_PER_CS_TRAINING_MULTICAST_EN,0);
+	ddr_setval_ach_as(_reg_PHY_PER_CS_TRAINING_EN, 1);
