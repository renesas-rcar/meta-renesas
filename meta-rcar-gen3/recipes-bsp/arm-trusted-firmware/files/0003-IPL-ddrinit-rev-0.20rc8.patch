+ ******************************************************************************/
+static uint32_t ddrphy_regif_chk(void)
+{
+	uint32_t tmp_ach[DRAM_CH_CNT];
+	uint32_t ch;
+	uint32_t err;
+	const uint32_t PI_VERSION_CODE=0x2041;
+
+	ddr_getval_ach(_reg_PI_VERSION, (uint32_t *)tmp_ach);
+	err = 0;
+	foreach_vch(ch) {
+		if(PI_VERSION_CODE!=tmp_ach[ch]) 
+			err = 1;
+	}
+	return err;
+}
