+		if(!(ch_have_this_cs[0] & (1<<ch))) dataL = dataL & 0xa;
+		if(!(ch_have_this_cs[1] & (1<<ch))) dataL = dataL & 0x5;
+		ddr_setval(ch, _reg_PI_CS_MAP, dataL);
