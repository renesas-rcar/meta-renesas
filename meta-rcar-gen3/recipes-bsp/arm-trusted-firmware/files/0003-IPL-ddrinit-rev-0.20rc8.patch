+	***********************************************************************/
+	/* search jedec_spec1 index */
+	for(i=JS1_USABLEC_SPEC_LO;i<JS1_FREQ_TBL_NUM-1;i++) {
+		if(js1[i].fx3 * 2 * ddr_mbpsdiv >= ddr_mbps*3)
+			break;
