+	CLKDEV_DEV_ID("dvc1", &mstp_clks[MSTP1018]),
+	CLKDEV_DEV_ID("scu", &mstp_clks[MSTP1017]),
+	CLKDEV_DEV_ID("ssi0", &mstp_clks[MSTP1015]),
+	CLKDEV_DEV_ID("ssi1", &mstp_clks[MSTP1014]),
+	CLKDEV_DEV_ID("ssi", &mstp_clks[MSTP1005]),
