+#ifdef CONFIG_MACH_APE6EVM
+#define SSR	0x14
+#define FTDR	0x20
+
+#define TDFE	(1 << 5)
+#define TEND	(1 << 6)
+
+#define SCIF_PHYS	0xe6c40000
+#define SCIF_VIRT	0xe6c40000
+#endif
+
+#ifdef CONFIG_MACH_LAGER
+#define SSR	0x10
+#define FTDR	0x0C
+
+#define TDFE	(1 << 5)
+#define TEND	(1 << 6)
+
+#define SCIF_PHYS	0xe6e60000
+#define SCIF_VIRT	0xe6e60000
+#endif
+
+	.macro	addruart, rp, rv, tmp
+	ldr	\rp, =SCIF_PHYS
+	ldr	\rv, =SCIF_VIRT
+	.endm
+
+	.macro	waituart, rd, rx
+1001:	ldrh	\rd, [\rx, #SSR]
+	tst	\rd, #TDFE
+	beq	1001b
+	.endm
+
+	.macro	senduart, rd, rx
+	strb	\rd, [\rx, #FTDR]
+	ldrh	\rd, [\rx, #SSR]
+	bic	\rd, \rd, #TEND
+	strh	\rd, [\rx, #SSR]
+	.endm
+
+	.macro	busyuart, rd, rx
+1001:	ldrh	\rd, [\rx, #SSR]
+	tst	\rd, #TEND
+	beq	1001b
+	.endm
