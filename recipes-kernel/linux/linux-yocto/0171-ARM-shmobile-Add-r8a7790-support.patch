+ * but WITHOUT ANY WARRANTY; without even the implied warranty of
+ * MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE. See the
+ * GNU General Public License for more details.
+ *
+ * You should have received a copy of the GNU General Public License
+ * along with this program; if not, write to the Free Software Foundation,
+ * Inc., 51 Franklin Street, Fifth Floor, Boston, MA  02110-1301, USA.
+ */
+
+#include <linux/kernel.h>
+#include <linux/init.h>
+#include <linux/delay.h>
+#include <linux/irq.h>
+#include <linux/platform_device.h>
+#include <linux/gpio.h>
+#include <linux/clk.h>
+#include <mach/r8a7790.h>
+#include <mach/common.h>
+#include <mach/irqs.h>
+#include <asm/mach-types.h>
+#include <asm/mach/arch.h>
+#include <asm/mach/map.h>
+#include <asm/mach/time.h>
+#include <asm/arch_timer.h>
+#include <asm/hardware/gic.h>
+
+static struct platform_device *lager_devices[] __initdata = {
+};
+
+static struct map_desc lager_io_desc[] __initdata = {
+	{
+		.virtual	= 0xe6000000,
+		.pfn		= __phys_to_pfn(0xe6000000),
+		.length		= SZ_16M,
+		.type		= MT_DEVICE_NONSHARED,
+	},
+};
+
+static void __init lager_map_io(void)
+{
+	iotable_init(lager_io_desc, ARRAY_SIZE(lager_io_desc));
+}
+
+static void __init lager_init_early(void)
+{
+	r8a7790_add_early_devices();
+}
+
+static void __init lager_init(void)
+{
+	r8a7790_pinmux_init();
+
+	r8a7790_add_standard_devices();
+	platform_add_devices(lager_devices, ARRAY_SIZE(lager_devices));
+}
+
+static void __init lager_timer_init(void)
+{
+	r8a7790_clock_init();
+	shmobile_timer.init();
+	return;
+}
+
+struct sys_timer lager_timer  = {
+	.init	= lager_timer_init,
+};
+
+static const char *lager_compat_dt[] __initdata = {
+	"renesas,lager",
+	NULL,
+};
+
+MACHINE_START(LAGER, "lager")
+	.init_machine	= lager_init,
+	.init_early	= lager_init_early,
+	.map_io		= lager_map_io,
+	.timer		= &lager_timer,
+	.init_irq	= r8a7790_init_irq,
+	.handle_irq	= gic_handle_irq,
+	.dt_compat	= lager_compat_dt,
+MACHINE_END
